program example2;
var P, Q, R, S, T: integer;
begin
    P := 3; Q := 7; R := 2; S := 5; T := 1;
    repeat
        if P < Q then
            while R >= S do
                if T = 1 then
                    R := R * 2
        else
            while P <= S do
                P := P + Q
    until Q > T
end.
